{
  "name": "keycloak-mcp",
  "version": "1.2.2",
  "description": "MCP server for Keycloak",
  "bin": {
    "keycloak-mcp": "./dist/server.js"
  },
  "main": "./dist/server.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "rimraf dist && tsc -p tsconfig.build.json && shx chmod +x dist/*.js",
    "prepare": "npm run build",
    "watch": "tsc --watch",
    "start": "node dist/server.js",
    "dev": "node --env-file .env --watch src/server.ts"
  },
  "keywords": [
    "keycloak",
    "mcp",
    "model-context-protocol",
    "keycloak-admin-client",
    "keycloak-mcp",
    "ai-agent",
    "mcpo",
    "ts-mcp-server",
    "mcp-server",
    "keycloak mcp"
  ],
  "license": "MIT",
  "author": "OUMERZOUG Haitham",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "git@github.com:HaithamOumerzoug/keycloak-mcp.git"
  },
  "homepage": "https://github.com/HaithamOumerzoug/keycloak-mcp#readme",
  "dependencies": {
    "@keycloak/keycloak-admin-client": "^26.2.4",
    "@modelcontextprotocol/sdk": "^1.12.0",
    "chalk": "^5.6.2",
    "zod": "^3.25.28"
  },
  "devDependencies": {
    "@types/node": "^22.15.21",
    "rimraf": "^6.0.1",
    "shx": "^0.4.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3"
  },
  "engines": {
    "node": ">=22.19.0"
  }
}
